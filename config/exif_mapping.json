      "default_strategy": "write_if_missing"
    },
    "geoData_latitude": {
      "source_fields": [
        "geoData.latitude"
      ],
      "target_tags": [
        "GPSLatitude"
      ],
      "default_strategy": "replace_all"
    },
    "geoData_longitude": {
      "source_fields": [
        "geoData.longitude"
      ],
      "target_tags": [
        "GPSLongitude"
      ],
      "default_strategy": "replace_all"
    },
    "geoData_altitude": {
      "source_fields": [
        "geoData.altitude"
      ],
      "target_tags": [
        "GPSAltitude"
      ],
      "default_strategy": "replace_all"
    },
    "geoData_latitude_ref": {
      "source_fields": [
        "geoData.latitude.ref"
      ],
      "target_tags": [
        "GPSLatitudeRef"
      ],
      "default_strategy": "replace_all",
      "value_mapping": {
        "positive": "N",
        "negative": "S"
      }
    },
    "geoData_longitude_ref": {
      "source_fields": [
        "geoData.longitude.ref"
      ],
      "target_tags": [
        "GPSLongitudeRef"
      ],
      "default_strategy": "replace_all",
      "value_mapping": {
