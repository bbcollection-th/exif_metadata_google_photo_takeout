{
  "metadata_mappings": {
    "title": {
      "source_fields": [
        "title"
      ],
      "target_tags": [
        "XMP:CustomTitle"
      ],
      "default_strategy": "write_if_missing"
    },
    "description": {
      "source_fields": [
        "description"
      ],
      "target_tags": [
        "EXIF:ImageDescription",
        "XMP-dc:Description",
        "IPTC:Caption-Abstract"
      ],
      "default_strategy": "preserve_existing",
      "sanitize": true
    },
    "imageviews": {
      "source_fields": [
        "imageViews"
      ],
      "target_tags": [
        "XMP:CustomImageviews"
      ],
      "default_strategy": "write_if_missing"
    },
    "creationTime": {
      "source_fields": [
        "creationTime"
      ],
      "target_tags": [
        "EXIF:DateTimeOriginal",
        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
    },
    "creationTime_timestamp": {
      "source_fields": [
        "creationTime.timestamp"
      ],
      "target_tags": [
        "EXIF:DateTimeOriginal",
        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
    },
    "creationTime_formatted": {
      "source_fields": [
        "creationTime.formatted"
      ],
      "target_tags": [
        "EXIF:DateTimeOriginal",
        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
    },
    "photoTakenTime": {
      "source_fields": [
        "photoTakenTime"
      ],
      "target_tags": [
        "EXIF:DateTimeOriginal",
        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
    },
    "photoTakenTime_timestamp": {
      "source_fields": [
        "photoTakenTime.timestamp"
      ],
      "target_tags": [
        "EXIF:DateTimeOriginal",
        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
    },
    "photoTakenTime_formatted": {
      "source_fields": [
        "photoTakenTime.formatted"
      ],
      "target_tags": [
        "EXIF:DateTimeOriginal",
        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
    },
    "geodata": {
      "source_fields": [
        "geoData"
      ],
      "target_tags": [
        "XMP:CustomGeodata"
      ],
      "default_strategy": "write_if_missing"
    },
    "geoData_latitude": {
      "source_fields": [
        "geoData.latitude"
      ],
      "target_tags": [
        "GPS:GPSLatitude",
        "GPS:GPSLongitude",
        "GPS:GPSAltitude"
      ],
      "default_strategy": "replace_all"
    },
    "geodata_longitude": {
      "source_fields": [
        "geoData.longitude"
      ],
      "target_tags": [
        "GPS:GPSLatitude",
        "GPS:GPSLongitude",
        "GPS:GPSAltitude"
      ],
      "default_strategy": "replace_all"
    },
    "geoData_altitude": {
      "source_fields": [
        "geoData.altitude"
      ],
      "target_tags": [
        "GPS:GPSLatitude",
        "GPS:GPSLongitude",
        "GPS:GPSAltitude"
      ],
      "default_strategy": "replace_all"
    },
    "geoData_latitudeSpan": {
      "source_fields": [
        "geoData.latitudeSpan"
      ],
      "target_tags": [
        "GPS:GPSLatitude",
        "GPS:GPSLongitude",
        "GPS:GPSAltitude"
      ],
      "default_strategy": "replace_all"
    },
    "geodata_longitudeSpan": {
      "source_fields": [
        "geoData.longitudeSpan"
      ],
      "target_tags": [
        "GPS:GPSLatitude",
        "GPS:GPSLongitude",
        "GPS:GPSAltitude"
      ],
      "default_strategy": "replace_all"
    },
    "url": {
      "source_fields": [
        "url"
      ],
      "target_tags": [
        "XMP:CustomUrl"
      ],
      "default_strategy": "write_if_missing"
    },
    "people": {
      "source_fields": [
        "people"
      ],
      "target_tags": [
        "XMP-iptcExt:PersonInImage"
      ],
      "default_strategy": "clean_duplicates",
      "normalize": "person_name"
    },
    "people_name": {
      "source_fields": [
        "people.name"
      ],
      "target_tags": [
        "XMP-iptcExt:PersonInImage"
      ],
      "default_strategy": "clean_duplicates",
      "normalize": "person_name"
    },
    "googlephotosorigin": {
      "source_fields": [
        "googlePhotosOrigin"
      ],
      "target_tags": [
        "XMP:CustomGooglephotosorigin"
      ],
      "default_strategy": "write_if_missing"
    },
    "googlephotosorigin_mobileupload": {
      "source_fields": [
        "googlePhotosOrigin.mobileUpload"
      ],
      "target_tags": [
        "XMP:CustomGooglephotosoriginmobileupload"
      ],
      "default_strategy": "write_if_missing"
    },
    "googlephotosorigin_mobileupload_devicefolder": {
      "source_fields": [
        "googlePhotosOrigin.mobileUpload.deviceFolder"
      ],
      "target_tags": [
        "XMP-dc:Subject",
        "IPTC:Keywords"
      ],
      "default_strategy": "clean_duplicates",
      "processing": {
        "prefix": "Album: ",
        "normalize": "keyword"
      }
    },
    "googlephotosorigin_mobileupload_devicefolder_localfoldername": {
      "source_fields": [
        "googlePhotosOrigin.mobileUpload.deviceFolder.localFolderName"
      ],
      "target_tags": [
        "XMP-dc:Subject",
        "IPTC:Keywords"
      ],
      "default_strategy": "clean_duplicates",
      "processing": {
        "prefix": "Album: ",
        "normalize": "keyword"
      }
    },
    "googlephotosorigin_mobileupload_devicetype": {
      "source_fields": [
        "googlePhotosOrigin.mobileUpload.deviceType"
      ],
      "target_tags": [
        "EXIF:Software",
        "XMP-xmp:CreatorTool"
      ],
      "default_strategy": "write_if_missing"
    },
    "archived": {
      "source_fields": [
        "archived"
      ],
      "target_tags": [
        "XMP:CustomArchived"
      ],
      "default_strategy": "write_if_missing"
    },
    "favorited": {
      "source_fields": [
        "favorited"
      ],
      "target_tags": [
        "XMP:Rating"
      ],
      "default_strategy": "preserve_existing",
      "value_mapping": {
        "true": "5",
        "false": null
      }
    },
    "trashed": {
      "source_fields": [
        "trashed"
      ],
      "target_tags": [
        "XMP:CustomTrashed"
      ],
      "default_strategy": "write_if_missing"
    },
    "access": {
      "source_fields": [
        "access"
      ],
      "target_tags": [
        "XMP:CustomAccess"
      ],
      "default_strategy": "write_if_missing"
    },
    "date": {
      "source_fields": [
        "date"
      ],
      "target_tags": [
        "EXIF:DateTimeOriginal",
        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
    },
    "date_timestamp": {
      "source_fields": [
        "date.timestamp"
      ],
      "target_tags": [
        "EXIF:DateTimeOriginal",
        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
    },
    "date_formatted": {
      "source_fields": [
        "date.formatted"
      ],
      "target_tags": [
        "EXIF:DateTimeOriginal",
        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
    }
  },
  "strategies": {
    "preserve_existing": {
      "description": "Préserver les métadonnées existantes",
      "exiftool_args": [
        "-wm",
        "cg"
      ]
    },
    "replace_all": {
      "description": "Remplacer toutes les métadonnées",
      "exiftool_args": [
        "-overwrite_original"
      ]
    },
    "write_if_missing": {
      "description": "Écrire seulement si absent",
      "condition_template": "-if \"not ${tag}\""
    },
    "clean_duplicates": {
      "description": "Nettoyer les doublons avant écriture",
      "pattern": [
        "${tag}-=${value}",
        "${tag}+=${value}"
      ]
    }
  },
  "global_settings": {
    "default_strategy": "write_if_missing",
    "efile_output_dir": "logs",
    "video_extensions": [
      ".mp4",
      ".mov",
      ".m4v",
      ".3gp"
    ],
    "common_args": [
      "-charset",
      "title=UTF8"
    ],
    "backup_original": true
  }
}
