    "googlephotosorigin_mobileupload": {
      "source_fields": [
        "googlePhotosOrigin.mobileUpload"
      ],
      "target_tags": [
        "XMP:CustomGooglephotosoriginmobileupload"
      ],
      "default_strategy": "write_if_missing"
    },
    "googlephotosorigin_mobileupload_devicefolder": {
      "source_fields": [
        "googlePhotosOrigin.mobileUpload.deviceFolder"
      ],
      "target_tags": [
        "XMP-dc:Subject",
        "IPTC:Keywords"
      ],
      "default_strategy": "clean_duplicates",
      "processing": {
        "prefix": "Album: ",
        "normalize": "keyword"
      }
    },
    "googlephotosorigin_mobileupload_devicefolder_localfoldername": {
      "source_fields": [
        "googlePhotosOrigin.mobileUpload.deviceFolder.localFolderName"
      ],
      "target_tags": [
        "XMP-dc:Subject",
        "IPTC:Keywords"
      ],
      "default_strategy": "clean_duplicates",
      "processing": {
        "prefix": "Album: ",
        "normalize": "keyword"
      }
    },
    "googlephotosorigin_mobileupload_devicetype": {
      "source_fields": [
        "googlePhotosOrigin.mobileUpload.deviceType"
      ],
      "target_tags": [
        "EXIF:Software",
        "XMP-xmp:CreatorTool"
      ],
      "default_strategy": "write_if_missing"
    },
    "archived": {
      "source_fields": [
        "archived"
      ],
      "target_tags": [
        "XMP:CustomArchived"
      ],
      "default_strategy": "write_if_missing"
    },
    "favorited": {
      "source_fields": [
        "favorited"
      ],
      "target_tags": [
        "XMP:Rating"
      ],
      "default_strategy": "preserve_positive_rating",
      "value_mapping": {
        "true": "5",
        "false": null
      }
    },
    "trashed": {
      "source_fields": [
        "trashed"
      ],
      "target_tags": [
        "XMP:CustomTrashed"
      ],
      "default_strategy": "write_if_missing"
    },
    "access": {
      "source_fields": [
        "access"
      ],
      "target_tags": [
        "XMP:CustomAccess"
      ],
      "default_strategy": "write_if_missing"
    },
    "date": {
      "source_fields": [
        "date"
      ],
      "target_tags": [
        "EXIF:DateTimeOriginal",
        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
    },
    "date_timestamp": {
      "source_fields": [
        "date.timestamp"
      ],
      "target_tags": [
        "EXIF:DateTimeOriginal",
        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
    },
    "date_formatted": {
      "source_fields": [
        "date.formatted"
      ],
      "target_tags": [
        "EXIF:DateTimeOriginal",
        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
    },
    "city": {
      "source_fields": [
        "city"
      ],
      "target_tags": [
        "XMP:City",
        "IPTC:City"
      ],
      "default_strategy": "write_if_missing"
    },
    "country": {
      "source_fields": [
        "country"
      ],
      "target_tags": [
        "XMP:Country",
        "IPTC:Country-PrimaryLocationName"
      ],
      "default_strategy": "write_if_missing"
    },
    "state": {
      "source_fields": [
        "state"
      ],
      "target_tags": [
        "XMP:State",
        "IPTC:Province-State"
      ],
      "default_strategy": "write_if_missing"
    },
    "place_name": {
      "source_fields": [
        "place_name"
      ],
      "target_tags": [
        "XMP:Location"
      ],
      "default_strategy": "write_if_missing"
    }
  },
  "strategies": {
    "preserve_existing": {
      "description": "Préserver les métadonnées existantes",
      "exiftool_args": [
        "-wm",
        "cg"
      ]
    },
    "replace_all": {
      "description": "Remplacer inconditionnellement la valeur",
      "pattern": ["-${tag}=${value}"]
    },
    "write_if_blank_or_missing": {
      "description": "Écrit seulement si le tag est inexistant ou vide (robuste multi-valué / lang-alt)",
      "condition_template": "-if not defined $${tag} or not length($${tag}) or not length($${tag}[0])",
      "pattern": ["-${tag}=${value}"]
    },
    "write_if_missing": {
      "description": "Écrire seulement si absent (ancienne version moins robuste)",
      "condition_template": "-if not $${tag}"
    },
    "clean_duplicates": {
      "description": "Nettoyer les doublons avant écriture",
      "pattern": [
        "${tag}-=${value}",
        "${tag}+=${value}"
      ]
    },
    "preserve_positive_rating": {
      "description": "Pour favorited: écrire Rating=5 si favorited=true ET (Rating absent OU Rating=0), ne jamais toucher si favorited=false",
      "condition_template": "-if not defined $${tag} or $${tag} eq '0'",
      "pattern": ["-${tag}=${value}"],
      "special_logic": "favorited_rating"
    }
  },
  "global_settings": {
    "default_strategy": "write_if_missing",
    "efile_output_dir": "logs",
    "video_extensions": [
      ".mp4",
      ".mov",
      ".m4v",
      ".3gp"
    ],
    "common_args": [
      "-charset",
      "filename=utf8",
      "-overwrite_original"
    ],
    "backup_original": true
  }
}
