        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
    },
    "photoTakenTime": {
      "source_fields": [
        "photoTakenTime"
      ],
      "target_tags": [
        "EXIF:DateTimeOriginal",
        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
    },
    "photoTakenTime_timestamp": {
      "source_fields": [
        "photoTakenTime.timestamp"
      ],
      "target_tags": [
        "EXIF:DateTimeOriginal",
        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
    },
    "photoTakenTime_formatted": {
      "source_fields": [
        "photoTakenTime.formatted"
      ],
      "target_tags": [
        "EXIF:DateTimeOriginal",
        "EXIF:CreateDate"
      ],
      "default_strategy": "replace_all",
      "format": "%Y:%m:%d %H:%M:%S"
