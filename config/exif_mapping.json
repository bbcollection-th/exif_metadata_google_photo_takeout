    },
    "albums_subjects": {
      "source_fields": [
        "albums"
      ],
      "target_tags": [
        "XMP-dc:Subject"
      ],
      "default_strategy": "clean_duplicates",
      "processing": {
        "prefix": "Album: ",
        "normalize": "keyword"
